{
  "version": 2,
  "builds": [
    {
      "src": "flashcard_cn.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/flashcard_cn.html"
    },
    {
      "src": "/flashcard",
      "dest": "/flashcard_cn.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
